// Windows 版本资源文件模板
#include <windows.h>

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @YUTILS_UTILS_VERSION_MAJOR@,@YUTILS_UTILS_VERSION_MINOR@,@YUTILS_UTILS_VERSION_PATCH@,0
 PRODUCTVERSION @YUTILS_UTILS_VERSION_MAJOR@,@YUTILS_UTILS_VERSION_MINOR@,@YUTILS_UTILS_VERSION_PATCH@,0
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x40004L
 FILETYPE 0x2L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "080404b0" // 中文(中国), Unicode
        BEGIN
            VALUE "CompanyName", "yUtils Project"
            VALUE "FileDescription", "yUtils Utility Library"
            VALUE "FileVersion", "@YUTILS_UTILS_VERSION@"
            VALUE "InternalName", "@UTILS_SDK_NAME@"
            VALUE "LegalCopyright", "Copyright (C) @YUTILS_UTILS_BUILD_DATE@"
            VALUE "OriginalFilename", "@UTILS_SDK_NAME@.dll"
            VALUE "ProductName", "yUtils SDK"
            VALUE "ProductVersion", "@YUTILS_UTILS_VERSION@"
            VALUE "BuildDate", "@YUTILS_UTILS_BUILD_DATE@"
            VALUE "BuildTime", "@YUTILS_UTILS_BUILD_TIME@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x804, 1200 // 中文(中国), Unicode
    END
END
// Windows 版本资源文件模板 - 用于earth SDK库
#include <windows.h>

// IDI_ICON1 ICON "logo.png"

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @EARTH_SDK_VERSION_MAJOR@,@EARTH_SDK_VERSION_MINOR@,@EARTH_SDK_VERSION_PATCH@,0
 PRODUCTVERSION @EARTH_SDK_VERSION_MAJOR@,@EARTH_SDK_VERSION_MINOR@,@EARTH_SDK_VERSION_PATCH@,0
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x40004L
 FILETYPE 0x2L  // 2L = 动态链接库
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "080404b0" // 中文(中国), Unicode
        BEGIN
            VALUE "CompanyName", "yAlgo Project"
            VALUE "FileDescription", "earth SDK Library"
            VALUE "FileVersion", "@EARTH_SDK_VERSION@"
            VALUE "InternalName", "libyalgo_earth"
            VALUE "LegalCopyright", "Copyright (C) @EARTH_SDK_BUILD_DATE@"
            VALUE "OriginalFilename", "libyalgo_earth.dll"
            VALUE "ProductName", "yAlgo SDK"
            VALUE "ProductVersion", "@EARTH_SDK_VERSION@"
            VALUE "BuildDate", "@EARTH_SDK_BUILD_DATE@"
            VALUE "BuildTime", "@EARTH_SDK_BUILD_TIME@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x804, 1200 // 中文(中国), Unicode
    END
END